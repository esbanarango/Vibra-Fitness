<%= simple_form_for ([@seat, @schedule]), :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :date %>
  <%= f.input :machine_num %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                seat_schedules_path(@seat), :class => 'btn' %>
  </div>
<% end %>
