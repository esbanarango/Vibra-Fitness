<%= simple_form_for @schedule, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :date %>
  <%= f.input :machine_num %>
  <%= f.input :seat_id %>
  <%= f.input :client_id %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                schedules_path, :class => 'btn' %>
  </div>
<% end %>
